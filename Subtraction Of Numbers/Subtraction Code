global _start

section .data
result db 0
newline db 10

section .text
_start:

    mov eax, 8     
    mov ebx, 1    
    sub eax, ebx    
    add eax, 48    
    mov [result], al 

    ; print result
    mov eax, 4
    mov ebx, 1
    mov ecx, result
    mov edx, 1
    int 80h

    ; print newline
    mov eax, 4
    mov ebx, 1
    mov ecx, newline
    mov edx, 1
    int 80h

    ; exit
    mov eax, 1
    xor ebx, ebx
    int 80h
